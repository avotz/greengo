@font-face {
	font-family: 'av';
	src:url('./fonts/av.eot?-pmmwod');
	src:url('./fonts/av.eot?#iefix-pmmwod') format('embedded-opentype'),
		url('./fonts/av.ttf?-pmmwod') format('truetype'),
		url('./fonts/av.woff?-pmmwod') format('woff'),
		url('./fonts/av.svg?-pmmwod#av') format('svg');
	font-weight: normal;
	font-style: normal;
}

[class^="icon-"], [class*=" icon-"] {
	font-family: 'av';
	speak: none;
	font-style: normal;
	font-weight: normal;
	font-variant: normal;
	text-transform: none;
	line-height: 1;

	/* Better Font Rendering =========== */
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
}

